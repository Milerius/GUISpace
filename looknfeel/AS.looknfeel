                <Image component="RightEdge" name="ASLoadingBar/ProgressEmptyR"/>
                <Image component="TopEdge" name="ASLoadingBar/ProgressEmptyT"/>
                <Image component="BottomEdge" name="ASLoadingBar/ProgressEmptyB"/>
                <Image component="Background" name="ASLoadingBar/ProgressEmptyFill"/>
            </FrameComponent>
        </ImagerySection>
        <ImagerySection name="progress_full">
            <FrameComponent>
                <Image component="TopLeftCorner" name="ASLoadingBar/ProgressFullTL"/>
                <Image component="TopRightCorner" name="ASLoadingBar/ProgressFullTR"/>
                <Image component="BottomLeftCorner" name="ASLoadingBar/ProgressFullBL"/>
                <Image component="BottomRightCorner" name="ASLoadingBar/ProgressFullBR"/>
                <Image component="LeftEdge" name="ASLoadingBar/ProgressFullL"/>
                <Image component="RightEdge" name="ASLoadingBar/ProgressFullR"/>
                <Image component="TopEdge" name="ASLoadingBar/ProgressFullT"/>
                <Image component="BottomEdge" name="ASLoadingBar/ProgressFullB"/>
                <Image component="Background" name="ASLoadingBar/ProgressFullFill"/>
            </FrameComponent>
        </ImagerySection>
        <ImagerySection name="progress_shadow">
            <ImageryComponent>
                <Area>
                    <Dim type="LeftEdge">
                        <OperatorDim op="Multiply">
                            <UnifiedDim scale="1" type="Width"/>
                            <PropertyDim name="CurrentProgress"/>
                        </OperatorDim>
                    </Dim>
                    <Dim type="RightEdge">
                        <OperatorDim op="Subtract">
                            <UnifiedDim scale="1" type="Width"/>
                            <AbsoluteDim value="5"/>
                        </OperatorDim>
                    </Dim>
                </Area>
                <Image name="AlfiskoSkin/ProgressShadow"/>
                <VertFormat type="Stretched"/>
            </ImageryComponent>
        </ImagerySection>
        <StateImagery name="Enabled">
            <Layer>
                <Section section="progress_empty"/>
                <Section section="progress_shadow"/>
            </Layer>
        </StateImagery>
        <StateImagery name="Disabled">
            <Layer>
                <Section section="progress_empty">
                    <Colours topLeft="FF7F7F7F" bottomLeft="FF7F7F7F" topRight="FF7F7F7F" bottomRight="FF7F7F7F"/>
                </Section>
            </Layer>
        </StateImagery>
        <StateImagery name="EnabledProgress">
            <Layer>
                <Section section="progress_full"/>
            </Layer>
        </StateImagery>
        <StateImagery name="DisabledProgress">
            <Layer>
                <Section section="progress_full">
                    <Colours topLeft="FF7F7F7F" bottomLeft="FF7F7F7F" topRight="FF7F7F7F" bottomRight="FF7F7F7F"/>
                </Section>
            </Layer>
        </StateImagery>
    </WidgetLook>
	
