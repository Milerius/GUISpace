<WidgetLook name="AS/ProgressBar">
        <Property name="CurrentProgress" value=".01"/>
        <Property name="Size" value="{{0, 256}, {0, 32}}" />
        <NamedArea name="ProgressArea"/>
        <ImagerySection name="progress_empty">
            <FrameComponent>
                <Image component="TopLeftCorner" name="ASLoadingBar/ProgressEmptyTL"/>
                <Image component="TopRightCorner" name="ASLoadingBar/ProgressEmptyTR"/>
